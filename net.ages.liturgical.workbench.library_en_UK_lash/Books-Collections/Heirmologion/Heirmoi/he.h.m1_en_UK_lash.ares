TinZoodochonPigin.mode = misc_en_UK_lash.Mode1
TinZoodochonPigin.ode = misc_en_UK_lash.Ode9
TinZoodochonPigin.text = "‘The ever-flowing life-bearing Fount, the light-bearing Lampstand of grace, the holy Temple, the all golden Tabernacle, wider than heaven and earth, the Mother of God, we faithful magnify‘."

ToFaeinonIminExanateilon.name = "Make the radiant. (NM)"
ToFaeinonIminExanateilon.incipit = "Make the radiant. …"
