hoMA.DoxologyGreat.title = "THE GREAT DOXOLOGY"
hoMA.MM.DoxologyGreat.text = ""
hoMA.DoxologyGreatIntro.text = "Glory to you who have shown us the light."
hoMA.DoxologyVerse01.text = "Glory to God in the highest, and on earth peace; good will among men." //le.go.os_en_UK_lash.leosMA.GreatDoxology.v01.text
hoMA.DoxologyVerse02.text = "We praise you, * we bless you, * we worship you, * we glorify you, * we give you thanks * for your great glory."
hoMA.DoxologyVerse03.text = "Lord, King, * God of heaven, * Father, Almighty, * Lord, only-begotten Son, * Jesus Christ * and holy Spirit."
hoMA.DoxologyVerse04.text = "Lord God, * Lamb of God, * Son of the Father, * who takes away the sin of the world, * have mercy on us; * you take away the sins of the world."
hoMA.DoxologyVerse05.text = "Receive our prayer, * you who sit at the right hand of the Father, * and have mercy on us."
hoMA.DoxologyVerse06.text = "For you alone are holy, * you alone are Lord, * Jesus Christ, * to the glory of God the Father. Amen."
hoMA.DoxologyVerse07.text = "Every day I will bless you, * and praise your name for ever * and to the ages of ages." //ps_en_UK_lash.psa144.v2.text
hoMA.DoxologyVerse07e.text = ""
hoMA.DoxologyVerse08.text = "Grant, Lord, * this day * to keep us without sin."
hoMA.DoxologyVerse08e.text = ""
hoMA.DoxologyVerse08n.text = ""
hoMA.DoxologyVerse09.text = "Blessed are you, O Lord, * the God of our Fathers, * and praised and glorified is your name * to the ages. Amen." //le.pr.os_en_UK_lash.leosMA.GreatDoxology.v09.text
hoMA.DoxologyVerse10.text = "May your mercy, O Lord, * be upon us, * as we have hoped in you." //ps_en_UK_lash.psa32.v22.text
hoMA.DoxologyVerse11a.text = "Blessed are you, O Lord, teach me your statutes." //ps_en_UK_lash.psa118.v12.text
hoMA.DoxologyVerse11b.text = "Blessed are you, O Lord, teach me your statutes."
hoMA.DoxologyVerse11c.text = "Blessed are you, O Lord, teach me your statutes."
hoMA.DoxologyVerse12a.text = "Lord, *you have been our refuge * from generation to generation." //ps_en_UK_lash.psa89.v1.text
hoMA.DoxologyVerse12b.text = "I said, * Lord, have mercy on me, * heal my soul, * for I have sinned against you." //ps_en_UK_lash.psa40.v5.text
hoMA.DoxologyVerse13.text = "Lord, I have run to you for refuge, * teach me to do your will, * for you are my God." //ps_en_UK_lash.psa142.v10alt.text
hoMA.DoxologyVerse14.text = "For with you is the source of life, * and in your light we shall see light." //ps_en_UK_lash.psa35.v10.text
hoMA.DoxologyVerse15.text = "Continue your mercy * towards those who know you." //ps_en_UK_lash.psa35.v11a.text
hoMA.DoxologyVerse16.text = ""
hoMA.DoxologyVerse17.text = ""
hoMA.DoxologyVerse18.text = ""
hoMA.DoxologyVerse19.text = "Holy Immortal, have mercy on us."
