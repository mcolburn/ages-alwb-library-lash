hoPCPrayer.Key131.title = ""
hoPCPrayer.Key131.text = "You see the deifying Blood: have fear."
hoPCPrayer.Key132.text = "It is a coal consuming the unworthy."
hoPCPrayer.Key133.text = "God's Body deifies and nourishes"
hoPCPrayer.Key134.text = "Me strangely; both my spirit and my mind."

