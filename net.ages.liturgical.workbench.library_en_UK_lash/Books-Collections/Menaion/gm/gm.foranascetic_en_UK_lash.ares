meMA.Ode4C1H.text = he.h.m8_en_UK_lash.EisakikoaKyrieTinAkoinSou.text

meMA.Ode4C11.ode = misc_en_UK_lash.Ode4
meMA.Ode4C11.mode = misc_en_UK_lash.Mode8
meMA.Ode4C11.melody = he.h.m8_en_UK_lash.EisakikoaKyrieTinAkoinSou.incipit
meMA.Ode4C11.incipit = "Your servant, Lord, thrust aside"
meMA.Ode4C11.text = "Your servant, Lord, thrust aside much-divided care, making his untroubled mind your dwelling."

meMA.Ode4C12.ode = misc_en_UK_lash.Ode4
meMA.Ode4C12.mode = misc_en_UK_lash.Mode8
meMA.Ode4C12.melody = he.h.m8_en_UK_lash.EisakikoaKyrieTinAkoinSou.incipit
meMA.Ode4C12.incipit = "Your delight, blessed Saint, was"
meMA.Ode4C12.text = "Your delight, blessed Saint, was in the commands of the Gospel; for taking up your cross you followed the Master rejoicing."

meMA.Ode4C13.ode = misc_en_UK_lash.Ode4
meMA.Ode4C13.mode = misc_en_UK_lash.Mode8
meMA.Ode4C13.melody = he.h.m8_en_UK_lash.EisakikoaKyrieTinAkoinSou.incipit
meMA.Ode4C13.incipit = "On wings of humility you"
meMA.Ode4C13.text = "On wings of humility you shattered the snares of the ill-intentioned; therefore, Venerable Father, you were exalted and declared a citizen of heaven."

meMA.Ode4C1T.ode = misc_en_UK_lash.Ode4
meMA.Ode4C1T.mode = misc_en_UK_lash.Mode8
meMA.Ode4C1T.melody = he.h.m8_en_UK_lash.EisakikoaKyrieTinAkoinSou.incipit
meMA.Ode4C1T.incipit = "Grant us pardon of offences,"
meMA.Ode4C1T.text = "Grant us pardon of offences, as you are sinless; make your world peaceful, O God, at the prayers of her who gave you birth."

meMA.Ode5C1H.ode = misc_en_UK_lash.Ode5
meMA.Ode5C1H.mode = misc_en_UK_lash.Mode8
meMA.Ode5C1H.melody = misc_en_UK_lash.HeirmosNotMetered
meMA.Ode5C1H.incipit = he.h.m8_en_UK_lash.OrthrizontesVoomenSoi.incipit
