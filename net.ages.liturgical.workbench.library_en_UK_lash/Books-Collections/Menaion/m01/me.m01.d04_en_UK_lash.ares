meMA.Ode1C11.ode=misc_en_UK_lash.Ode1
meMA.Ode1C11.mode=misc_en_UK_lash.Mode6
meMA.Ode1C11.melody=he.h.m6_en_UK_lash.OsEnIpeiroPezefsas.incipit
meMA.Ode1C11.incipit = "Let creation rejoice, let the"
meMA.Ode1C11.text="Let creation rejoice, let the families of the Nations sing: Jesus, the King of all the earth, is approaching the streams of Jordan to bring bout our cleansing."

meMA.Ode1C12.ode=misc_en_UK_lash.Ode1
meMA.Ode1C12.mode=misc_en_UK_lash.Mode6
meMA.Ode1C12.melody=he.h.m6_en_UK_lash.OsEnIpeiroPezefsas.incipit
meMA.Ode1C12.incipit = "Clothed in the royal purple"
meMA.Ode1C12.text="Clothed in the royal purple from virgin blood, O Lord, you are entering the streams of the river naked, as you clothe our nakedness."

meMA.Ode1C13.ode=misc_en_UK_lash.Ode1
meMA.Ode1C13.mode=misc_en_UK_lash.Mode6
meMA.Ode1C13.melody=he.h.m6_en_UK_lash.OsEnIpeiroPezefsas.incipit
meMA.Ode1C13.incipit = "Offspring though you are of"
meMA.Ode1C13.text="Offspring though you are of the Father without beginning, O Compassionate, you are approaching simply the offspring of Zachary requesting baptism, that you make us children of God by grace."

