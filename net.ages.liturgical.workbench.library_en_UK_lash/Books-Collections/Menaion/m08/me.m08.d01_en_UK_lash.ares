meHO.Month.text = "AUGUST"
meHO.DaysHours.text = "WHICH HAS THIRTY ONE DAYS"

meDate.text = "ON THE 1ST OF THE MONTH"
meCommemoration.text = "Commemoration of the seven holy Martyrs, the Maccabees, and of their teacher Eleazar and their mother Solomoni and the Procession of the precious and life-giving Cross." // full title of day
meShortCommemoration1.text = ""
meInsert0 = ""
