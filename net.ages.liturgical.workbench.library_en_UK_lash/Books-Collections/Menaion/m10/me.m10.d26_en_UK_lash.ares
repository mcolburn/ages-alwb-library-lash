meMA.Ode5C2T.mode = miscellanea_en_UK_lash.misc.Mode4
meMA.Ode5C2T.melody = he.h.m4_en_UK_lash.heHE.SyKyrieMouFos.name
meMA.Ode5C2T.incipit = "The Word of God, consubstantial…"
meMA.Ode5C2T.text = "The Word of God, consubstantial with the Father, O Mother of God, beyond understanding and reason became your Son, consubstantial with you."

