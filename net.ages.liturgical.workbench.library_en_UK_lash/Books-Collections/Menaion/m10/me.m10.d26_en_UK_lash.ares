meMA.Ode6C22.mode = miscellanea_en_UK_lash.misc.Mode4
meMA.Ode6C22.melody = he.h.m4_en_UK_lash.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C22.incipit = "Loose the bonds of my…"
meMA.Ode6C22.text = "Loose the bonds of my faults by your prayers, for as an unconquerable Martyr you have freedom to speak before the Master. And be for me refuge an protection, Demetrios."

meMA.Ode6C2T.ode = miscellanea_en_UK_lash.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_en_UK_lash.misc.Mode4
