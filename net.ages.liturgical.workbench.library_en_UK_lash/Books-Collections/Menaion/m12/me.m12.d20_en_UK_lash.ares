meVE.Aposticha1.poet = poets_en_UK_lash.Kyprianou
meVE.Aposticha1.mode = misc_en_UK_lash.Mode2
meVE.Aposticha1.melody = ""
meVE.Aposticha1.incipit = "Bethlehem, land ofJuda, city according"
meVE.Aposticha1.text = "Bethlehem, land ofJuda, city according to the flesh, radiantly make ready the divine Cave, in which God comes to birth in flesh from a Holy Virgin who knows not man, to save our race."

meVE.Aposticha2.poet = poets_en_UK_lash.AndreouPyrou
meVE.Aposticha2.mode = misc_en_UK_lash.Mode2
meVE.Aposticha2.melody = ""
meVE.Aposticha2.incipit = "Come, let us all faithfully"
meVE.Aposticha2.text = "Come, let us all faithfully celebrate the forefeast of the Nativity of Christ; and spiritually setting forth our hymn like a star, let us with the Shepherds shout aloud the Magis’ hymn of glory, ‘The salvation of mortals has come from a virgin womb, to call back believers’."

meVE.Aposticha3.poet = poets_en_UK_lash.Kyprianou
meVE.Aposticha3.mode = misc_en_UK_lash.Mode2
meVE.Aposticha3.melody = misc_en_UK_lash.AutomelonNotMetered
meVE.Aposticha3.incipit = he.a.m2_en_UK_lash.OikosTouEfratha.incipit
meVE.Aposticha3.text = he.a.m2_en_UK_lash.OikosTouEfratha.text

meVE.AposGlory.poet = poets_en_UK_lash.Stouditou
meVE.AposGlory.mode = misc_en_UK_lash.Mode1
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "How firm and adamantine was"
meVE.AposGlory.text = "How firm and adamantine was your soul, most blessed Ignatios! For as you had an unyielding desire for your true lover, you said, ‘There is in me no matter-loving fire, but rather living water, which speaks in me, and from within says to me: Come to the Father’; therefore, fired by the divine Spirit, you roused wild beasts to part you quickly from the world and send you to Christ the beloved; implore him that our souls may be saved."

meVE.AposBoth.poet = poets_en_UK_lash.Kyprianou
meVE.AposBoth.mode = misc_en_UK_lash.Mode2
meVE.AposBoth.melody = ""
meVE.AposBoth.incipit = me.m12.d12_en_UK_lash.meVE.AposBoth.incipit
meVE.AposBoth.text = me.m12.d12_en_UK_lash.meVE.AposBoth.text

meVE.Apolytikion1.mode = misc_en_UK_lash.Mode4
meVE.Apolytikion1.melody = he.a.m4_en_UK_lash.KateplagiIosif.notmetered
meVE.Apolytikion1.incipit = "Bethlehem prepare; Eden is opened"
meVE.Apolytikion1.text = "Bethlehem prepare; Eden is opened for all. Make ready Ephratha, because the tree of life has flowered in the Cave from the Virgin. For her womb has been revealed as the spiritual Paradise in which is the plant of life; eating from it we shall live; we shall not die as Adam. Christ is born to raise up his image which had fallen."

