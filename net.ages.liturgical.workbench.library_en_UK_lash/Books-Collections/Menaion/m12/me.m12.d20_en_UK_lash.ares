meMA.Ode6C11.incipit = "Heaven upon earth shows to"
meMA.Ode6C11.text = "Heaven upon earth shows to the Astrologers through a shining star the heavenly King now being brought to birth for our sake in the city of David."

meMA.Ode6C12.ode = misc_en_UK_lash.Ode6
