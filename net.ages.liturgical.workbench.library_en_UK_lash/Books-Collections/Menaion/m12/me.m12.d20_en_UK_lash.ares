meMA.Exaposteilarion1.mode = misc_en_UK_lash.Mode3
meMA.Exaposteilarion1.melody = he.a.m3_en_UK_lash.EpeskepsatoImas.notmetered
meMA.Exaposteilarion1.incipit = "Prepare Bethlehem; Cave make ready;"
meMA.Exaposteilarion1.text = "Prepare Bethlehem; Cave make ready; and Manger receive the uncircumscribed God, circumscribed in flesh; for he is coming to be born, as he has been well pleased."

meMA.Exaposteilarion2.mode = misc_en_UK_lash.Mode2
meMA.Exaposteilarion2.melody = he.a.m2_en_UK_lash.ToisMathitaisSynelthomen.notmetered
meMA.Exaposteilarion2.incipit = "I am wheat of God"
meMA.Exaposteilarion2.text = "I am wheat of God and the Saviour, you said, Godbearer Ignatios, and now I must be ground by the teeth of wild beasts, so I shall be seen as sweet and pure bread for the Holy Trinity; as you stand in its presence may you remember those who celebrate your light-bearing memorial, all-wise High Priest and Martyr."

meMA.Exaposteilarion3.mode = misc_en_UK_lash.Mode2
meMA.Exaposteilarion3.melody = he.a.m2_en_UK_lash.GynaikesAkoutisthite.notmetered
meMA.Exaposteilarion3.incipit = "As wounded by the affection"
meMA.Exaposteilarion3.text = "As wounded by the affection of love of your Lord, you cried out: Christ my love has been willingly crucified; and so I thirst to be made thirsty, and as I live my love is to be loved; I long to have become in his passion and his kingdom."

meMA.Exaposteilarion4.mode = misc_en_UK_lash.Mode2
meMA.Exaposteilarion4.melody = he.a.m2_en_UK_lash.GynaikesAkoutisthite.notmetered
meMA.Exaposteilarion4.incipit = "Believers, let us now with"
meMA.Exaposteilarion4.text = "Believers, let us now with faith make songs resound for the Forefeast of the Nativity; for Christ is coming, has come to be born in Bethlehem as a mortal from a Virgin, and to appear as an infant, wrapped in swaddling clothes, he who is before the ages."

meMA.Lauds1.poet = poets_en_UK_lash.RomanouTouMelodou
meMA.Lauds1.mode = misc_en_UK_lash.Mode6
