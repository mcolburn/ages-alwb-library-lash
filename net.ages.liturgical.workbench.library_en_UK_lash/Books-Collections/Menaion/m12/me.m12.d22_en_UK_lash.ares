meVE.Aposticha1.mode = misc_en_UK_lash.Mode2
meVE.Aposticha1.melody = he.a.m2_en_UK_lash.OikosTouEfratha.notmetered
meVE.Aposticha1.incipit = "Christ is coming to crush…"
meVE.Aposticha1.text = "Christ is coming to crush the evil one, to enlighten those in darkness and release the prisoners; let us go forth to meet him."

meVE.Aposticha2.mode = misc_en_UK_lash.Mode2
meVE.Aposticha2.melody = he.a.m2_en_UK_lash.OikosTouEfratha.notmetered
meVE.Aposticha2.incipit = "Sing families of nations praise…"
meVE.Aposticha2.text = "Sing families of nations praise and glory; Magi with gifts, Shepherds abiding in the fields eagerly hurry onwards."

meVE.Aposticha3.mode = misc_en_UK_lash.Mode2
meVE.Aposticha3.melody = he.a.m2_en_UK_lash.OikosTouEfratha.notmetered
meVE.Aposticha3.incipit = "Mountains and hills, valleys and…"
meVE.Aposticha3.text = "Mountains and hills, valleys and plains, rivers and all creation now magnify the Creator who is brought to birth."

