peMA.Ode4C1H.ode = misc_en_UK_lash.Ode4
peMA.Ode4C1H.mode = misc_en_UK_lash.Mode3
peMA.Ode4C1H.melody = he.h.m3_en_UK_lash.ToKataskionOros.melody
peMA.Ode4C1H.incipit = he.h.m3_en_UK_lash.ToKataskionOros.incipit
peMA.Ode4C1H.text = he.h.m3_en_UK_lash.ToKataskionOros.text

peMA.Ode4C11.ode = misc_en_UK_lash.Ode4
peMA.Ode4C11.mode = misc_en_UK_lash.Mode3
peMA.Ode4C11.melody = he.h.m3_en_UK_lash.ToKataskionOros.name
peMA.Ode4C11.incipit = "Consumed by envy the Hebrew…"
peMA.Ode4C11.text = "Consumed by envy the Hebrew people crucified you on a tree, O Lord; and having annulled the sentence of death, you rose from the dead as mighty, raising the world with you."

peMA.Ode4C12.ode = misc_en_UK_lash.Ode4
peMA.Ode4C12.mode = misc_en_UK_lash.Mode3
peMA.Ode4C12.melody = he.h.m3_en_UK_lash.ToKataskionOros.name
peMA.Ode4C12.incipit = "'Why, O women, do you…"
peMA.Ode4C12.text = "'Why, O women, do you seek the inexhaustible Sweetness with sweet spices? he has risen', the one who was seated clothed in white said to the Myrrhbearers, 'and fills the whole world with fragrance."

peMA.Ode4C13.ode = misc_en_UK_lash.Ode4
peMA.Ode4C13.mode = misc_en_UK_lash.Mode3
peMA.Ode4C13.melody = he.h.m3_en_UK_lash.ToKataskionOros.name
peMA.Ode4C13.incipit = "Bearing the image of a…"
peMA.Ode4C13.text = "Bearing the image of a servant in your surpassing compassion, O Word, you passed by on your way and restored health to the man who had lain sick for many years, and ordered him to take up his bed."

peMA.Ode4C1F1.ode = misc_en_UK_lash.Ode4
peMA.Ode4C1F1.mode = misc_en_UK_lash.Mode3
peMA.Ode4C1F1.melody = he.h.m3_en_UK_lash.ToKataskionOros.name
peMA.Ode4C1F1.incipit = "An Angel of the Lord…"
peMA.Ode4C1F1.text = "An Angel of the Lord went down at a certain time and stirred up the water in the Sheep Pool, making one, and only one person well; but Christ saves countless multitudes by divine Baptism."

peMA.Ode4C1F2.ode = misc_en_UK_lash.Ode4
peMA.Ode4C1F2.mode = misc_en_UK_lash.Mode3
peMA.Ode4C1F2.melody = he.h.m3_en_UK_lash.ToKataskionOros.name
peMA.Ode4C1F2.incipit = "Prince of the Angels, guide…"
peMA.Ode4C1F2.text = "Prince of the Angels, guide of the erring, God's Chief Captain, be among us at this hour, and offer all our prayers to the only Maker and Master."

peMA.Ode4C1Tr.ode = misc_en_UK_lash.Ode4
peMA.Ode4C1Tr.mode = misc_en_UK_lash.Mode3
peMA.Ode4C1Tr.melody = he.h.m3_en_UK_lash.ToKataskionOros.name
peMA.Ode4C1Tr.incipit = "The holy Trinity is united…"
peMA.Ode4C1Tr.text = "The holy Trinity is united in nature, divided in persons: the Father beyond all being, and the co-eternal Son and the Holy and alone all-powerful Spirit."

peMA.Ode4C1T.ode = misc_en_UK_lash.Ode4
peMA.Ode4C1T.mode = misc_en_UK_lash.Mode3
peMA.Ode4C1T.melody = he.h.m3_en_UK_lash.ToKataskionOros.name
peMA.Ode4C1T.incipit = "How did you give suck…"
peMA.Ode4C1T.text = "How did you give suck, O pure one? How give birth to an infant more ancient than Adam? How bear in your embrace as Son him who is borne on the shoulders of the Cherubim? As he understands, as he knows, he who gave everything being."

peMA.Ode5C1H.ode = misc_en_UK_lash.Ode5
peMA.Ode5C1H.mode = misc_en_UK_lash.Mode3
peMA.Ode5C1H.melody = he.h.m3_en_UK_lash.FotiToAnespero.melody
peMA.Ode5C1H.incipit = he.h.m3_en_UK_lash.FotiToAnespero.incipit
peMA.Ode5C1H.text = he.h.m3_en_UK_lash.FotiToAnespero.text

peMA.Ode5C11.ode = misc_en_UK_lash.Ode5
peMA.Ode5C11.mode = misc_en_UK_lash.Mode3
peMA.Ode5C11.melody = he.h.m3_en_UK_lash.FotiToAnespero.name
peMA.Ode5C11.incipit = "You were lifted up on…"
peMA.Ode5C11.text = "You were lifted up on the tree of the Cross and you lifted up the whole world with you; and among the dead as God you raise those who have been dead since time began."

peMA.Ode5C12.ode = misc_en_UK_lash.Ode5
peMA.Ode5C12.mode = misc_en_UK_lash.Mode3
peMA.Ode5C12.melody = he.h.m3_en_UK_lash.FotiToAnespero.name
peMA.Ode5C12.incipit = "Christ has risen as he…"
peMA.Ode5C12.text = "Christ has risen as he said, having emptied the palaces of Hell, and he appears to the Apostles, giving them a share in the joy that lasts forever."

peMA.Ode5C13.ode = misc_en_UK_lash.Ode5
peMA.Ode5C13.mode = misc_en_UK_lash.Mode3
peMA.Ode5C13.melody = he.h.m3_en_UK_lash.FotiToAnespero.name
peMA.Ode5C13.incipit = "A snow-white, dazzling Angel appeared…"
peMA.Ode5C13.text = "A snow-white, dazzling Angel appeared to the women saying, 'Do not weep. Our life has arisen, having given life to the dead in the graves'."

peMA.Ode5C1F1.ode = misc_en_UK_lash.Ode5
peMA.Ode5C1F1.mode = misc_en_UK_lash.Mode3
peMA.Ode5C1F1.melody = he.h.m3_en_UK_lash.FotiToAnespero.name
peMA.Ode5C1F1.incipit = "As you raised the Paralysed…"
peMA.Ode5C1F1.text = "As you raised the Paralysed Man, O Christ, heal my soul crippled by transgressions, and lead me aright to walk in your straight paths."

peMA.Ode5C1F2.ode = misc_en_UK_lash.Ode5
peMA.Ode5C1F2.mode = misc_en_UK_lash.Mode3
peMA.Ode5C1F2.melody = he.h.m3_en_UK_lash.FotiToAnespero.name
peMA.Ode5C1F2.incipit = "Michael, Prince of Angels, save…"
peMA.Ode5C1F2.text = "Michael, Prince of Angels, save the people which throngs your temple today and proclaims the wonderful works of God."

peMA.Ode5C1Tr.ode = misc_en_UK_lash.Ode5
peMA.Ode5C1Tr.mode = misc_en_UK_lash.Mode3
peMA.Ode5C1Tr.melody = he.h.m3_en_UK_lash.FotiToAnespero.name
peMA.Ode5C1Tr.incipit = "Equal in honour, triple Unity…"
peMA.Ode5C1Tr.text = "Equal in honour, triple Unity, separated faithfully in persons but united in nature, are Father, Son and the divine Spirit."

peMA.Ode5C1T.ode = misc_en_UK_lash.Ode5
peMA.Ode5C1T.mode = misc_en_UK_lash.Mode3
peMA.Ode5C1T.melody = he.h.m3_en_UK_lash.FotiToAnespero.name
peMA.Ode5C1T.incipit = "We hymn your child-bearing without…"
peMA.Ode5C1T.text = "We hymn your child-bearing without seed and your birth-giving beyond understanding, O Pure one, calling you blessed as Mother of the Maker of all things and Master."

peMA.Ode6C1H.ode = misc_en_UK_lash.Ode6
peMA.Ode6C1H.mode = misc_en_UK_lash.Mode3
peMA.Ode6C1H.melody = he.h.m3_en_UK_lash.VythosMoiTonPathon.melody
peMA.Ode6C1H.incipit = he.h.m3_en_UK_lash.VythosMoiTonPathon.incipit
peMA.Ode6C1H.text = he.h.m3_en_UK_lash.VythosMoiTonPathon.text

peMA.Ode6C11.ode = misc_en_UK_lash.Ode6
peMA.Ode6C11.mode = misc_en_UK_lash.Mode3
peMA.Ode6C11.melody = he.h.m3_en_UK_lash.VythosMoiTonPathon.name
peMA.Ode6C11.incipit = "Willingly you were raised on…"
peMA.Ode6C11.text = "Willingly you were raised on a tree, you were placed as a dead man in a tomb, and having given life to all the dead in Hell together, you rose, O Christ, by divine power."

peMA.Ode6C12.ode = misc_en_UK_lash.Ode6
peMA.Ode6C12.mode = misc_en_UK_lash.Mode3
peMA.Ode6C12.melody = he.h.m3_en_UK_lash.VythosMoiTonPathon.name
peMA.Ode6C12.incipit = "Hell meeting you below was…"
peMA.Ode6C12.text = "Hell meeting you below was embittered, Merciful one, and hastily gave back his prisoners, as they hymned your dread Resurrection, Saviour, with never silent voices."

peMA.Ode6C13.ode = misc_en_UK_lash.Ode6
peMA.Ode6C13.mode = misc_en_UK_lash.Mode3
peMA.Ode6C13.melody = he.h.m3_en_UK_lash.VythosMoiTonPathon.name
peMA.Ode6C13.incipit = "The divine Disciples, when they…"
peMA.Ode6C13.text = "The divine Disciples, when they saw the life of all come from the grave, with great desire, right judgement and gladness of soul worshipped the risen Christ."

peMA.Ode6C1F1.ode = misc_en_UK_lash.Ode6
peMA.Ode6C1F1.mode = misc_en_UK_lash.Mode3
peMA.Ode6C1F1.melody = he.h.m3_en_UK_lash.VythosMoiTonPathon.name
peMA.Ode6C1F1.incipit = "He who of old had…"
peMA.Ode6C1F1.text = "He who of old had lain for many years on a bed of pain, at your order, O Christ, was made whole, and with hymns glorified your compassion, O Giver of life."

peMA.Ode6C1F2.ode = misc_en_UK_lash.Ode6
peMA.Ode6C1F2.mode = misc_en_UK_lash.Mode3
peMA.Ode6C1F2.melody = he.h.m3_en_UK_lash.VythosMoiTonPathon.name
peMA.Ode6C1F2.incipit = "Michael, Prince of Angels, who…"
peMA.Ode6C1F2.text = "Michael, Prince of Angels, who stand by the throne of the Master, be present among us now and guide us who have gained you as a fervent protector towards paths of life."

peMA.Ode6C1Tr.ode = misc_en_UK_lash.Ode6
peMA.Ode6C1Tr.mode = misc_en_UK_lash.Mode3
peMA.Ode6C1Tr.melody = he.h.m3_en_UK_lash.VythosMoiTonPathon.name
peMA.Ode6C1Tr.incipit = "With the dread armies above…"
peMA.Ode6C1Tr.text = "With the dread armies above, I reverence you, Trinity in persons; I proclaim you, Unity in Being, O Father without beginning, Son, right Spirit, God of all."

peMA.Ode6C1T.ode = misc_en_UK_lash.Ode6
peMA.Ode6C1T.mode = misc_en_UK_lash.Mode3
peMA.Ode6C1T.melody = he.h.m3_en_UK_lash.VythosMoiTonPathon.name
peMA.Ode6C1T.incipit = "He who bears all things…"
peMA.Ode6C1T.text = "He who bears all things by divine spirit is held, Virgin Mother of God, in your arms. He snatches us all from the enslaving hand of the foe, for he is compassionate."

peMA.Kontakion1.mode = misc_en_UK_lash.Mode3
peMA.Kontakion1.melody = he.a.m3_en_UK_lash.IParthenosSimeron.notmetered
peMA.Kontakion1.incipit = "By your divine presence, O…"
peMA.Kontakion1.text = "By your divine presence, O Lord, raise my soul, grievously paralysed by sins of every kind and by unnatural deeds, as you also raised the Paralysed Man of old, that saved I may cry to you: O merciful Christ, glory to your might."

peMA.Oikos1.text = "You grasp the ends of the earth in the hollow of your hand, Jesus, God without beginning with the Father, Master of all things with the Holy Spirit; you appeared in flesh, healing diseases and banishing sufferings, giving light to the blind, and with a divine word you raised up the Paralysed Man, ordering him to walk quickly and to take up on his shoulders the bed which had borne him; therefore with him we all sing praises and cry out: O merciful Christ, glory to your might."

