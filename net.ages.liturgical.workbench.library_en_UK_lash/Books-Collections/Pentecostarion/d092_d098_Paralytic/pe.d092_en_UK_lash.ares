peMA.C1.verse = prayers_en_UK_lash.verse_God.text

peMA.Ode1C11.ode = misc_en_UK_lash.Ode1
peMA.Ode1C11.mode = misc_en_UK_lash.Mode3
peMA.Ode1C11.melody = he.h.m3_en_UK_lash.ThavmastosEndoxos.name
peMA.Ode1C11.incipit = "O only King, who perform…"
peMA.Ode1C11.text = "O only King, who perform signs and do marvels, as merciful you willingly underwent the Cross; having put death to death by death, you have given us life."

peMA.Ode1C12.ode = misc_en_UK_lash.Ode1
peMA.Ode1C12.mode = misc_en_UK_lash.Mode3
peMA.Ode1C12.melody = he.h.m3_en_UK_lash.ThavmastosEndoxos.name
peMA.Ode1C12.incipit = "At Christ's Rising let us…"
peMA.Ode1C12.text = "At Christ's Rising let us dance with faith today, O peoples: Hell has been despoiled; rapidly he gave up the prisoners he held, as they hymned the wonderful works of God."

peMA.Ode1C13.ode = misc_en_UK_lash.Ode1
peMA.Ode1C13.mode = misc_en_UK_lash.Mode3
peMA.Ode1C13.melody = he.h.m3_en_UK_lash.ThavmastosEndoxos.name
peMA.Ode1C13.incipit = "O Christ, who once with…"
peMA.Ode1C13.text = "O Christ, who once with divine power braced the limbs of the paralytic by your word and ordered him who had been weak for many years to take up his bed, heal my fearsomely sick soul."

peMA.Ode1C1F1.ode = misc_en_UK_lash.Ode1
peMA.Ode1C1F1.mode = misc_en_UK_lash.Mode3
peMA.Ode1C1F1.melody = he.h.m3_en_UK_lash.ThavmastosEndoxos.name
peMA.Ode1C1F1.incipit = "Once the Angel would come…"
peMA.Ode1C1F1.text = "Once the Angel would come down into the Sheep Pool and cure one person each year; but now by divine Baptism Christ cleanses unnumbered multitudes."

peMA.Ode1C1F2.ode = misc_en_UK_lash.Ode1
peMA.Ode1C1F2.mode = misc_en_UK_lash.Mode3
peMA.Ode1C1F2.melody = he.h.m3_en_UK_lash.ThavmastosEndoxos.name
peMA.Ode1C1F2.incipit = "Prince of Angels, Chief Captain…"
peMA.Ode1C1F2.text = "Prince of Angels, Chief Captain of the ministers on high, protect, and guard those who have come together in your venerable temple and who sing God's praises."

peMA.Ode1C1Tr.ode = misc_en_UK_lash.Ode1
peMA.Ode1C1Tr.mode = misc_en_UK_lash.Mode3
peMA.Ode1C1Tr.melody = he.h.m3_en_UK_lash.ThavmastosEndoxos.name
peMA.Ode1C1Tr.incipit = "With the Bodiless ones let…"
peMA.Ode1C1Tr.text = "With the Bodiless ones let us ceaselessly sing the praise of God without beginning in three persons but one nature, Father, Word, Spirit, with kingdom and might undivided."

peMA.Ode1C1T.ode = misc_en_UK_lash.Ode1
peMA.Ode1C1T.mode = misc_en_UK_lash.Mode3
peMA.Ode1C1T.melody = he.h.m3_en_UK_lash.ThavmastosEndoxos.name
peMA.Ode1C1T.incipit = "O holy one, ever save…"
peMA.Ode1C1T.text = "O holy one, ever save this your city, which honours you, from dangers, capture by aliens, civil war, the sword and every other threat."

peMA.Ode3C1H.ode = misc_en_UK_lash.Ode3
peMA.Ode3C1H.mode = misc_en_UK_lash.Mode3
peMA.Ode3C1H.melody = he.h.m3_en_UK_lash.SteiraPsychi.melody
peMA.Ode3C1H.incipit = he.h.m3_en_UK_lash.SteiraPsychi.incipit
peMA.Ode3C1H.text = he.h.m3_en_UK_lash.SteiraPsychi.text

peMA.Ode3C11.ode = misc_en_UK_lash.Ode3
peMA.Ode3C11.mode = misc_en_UK_lash.Mode3
peMA.Ode3C11.melody = he.h.m3_en_UK_lash.SteiraPsychi.name
peMA.Ode3C11.incipit = "The sun of old seeing…"
peMA.Ode3C11.text = "The sun of old seeing you hanging upon the tree, O Word, withdrew its light, the whole earth was shaken; when you died the dead arose, O all-powerful."

peMA.Ode3C12.ode = misc_en_UK_lash.Ode3
peMA.Ode3C12.mode = misc_en_UK_lash.Mode3
peMA.Ode3C12.melody = he.h.m3_en_UK_lash.SteiraPsychi.name
peMA.Ode3C12.incipit = "When you came with your…"
peMA.Ode3C12.text = "When you came with your soul to the belly of the earth, Hell speedily gave up the souls he had acquired, which loudly sang a song of thanks to your might, O only Lord."

peMA.Ode3C1F1.ode = misc_en_UK_lash.Ode3
peMA.Ode3C1F1.mode = misc_en_UK_lash.Mode3
peMA.Ode3C1F1.melody = he.h.m3_en_UK_lash.SteiraPsychi.name
peMA.Ode3C1F1.incipit = "O most Good, heal my…"
peMA.Ode3C1F1.text = "O most Good, heal my soul which for many many years has been grievously sick; that I may tread the paths which you have marked out for those who long for you."

peMA.Ode3C1F2.ode = misc_en_UK_lash.Ode3
peMA.Ode3C1F2.mode = misc_en_UK_lash.Mode3
peMA.Ode3C1F2.melody = he.h.m3_en_UK_lash.SteiraPsychi.name
peMA.Ode3C1F2.incipit = "With the ranks on high…"
peMA.Ode3C1F2.text = "With the ranks on high make intercession, O Archangel of God, for those who hymn you in faith, guarding and protecting us who fall into the snares of life's passions."

peMA.Ode3C1Tr.ode = misc_en_UK_lash.Ode3
peMA.Ode3C1Tr.mode = misc_en_UK_lash.Mode3
peMA.Ode3C1Tr.melody = he.h.m3_en_UK_lash.SteiraPsychi.name
peMA.Ode3C1Tr.incipit = "Glory let us cry aloud…"
peMA.Ode3C1Tr.text = "Glory let us cry aloud to Father, Son and Spirit: for God is one by nature, whom all the powers of heaven glorify in fear, crying: Holy, Holy, Holy!"

peMA.Ode3C1T.ode = misc_en_UK_lash.Ode3
peMA.Ode3C1T.mode = misc_en_UK_lash.Mode3
peMA.Ode3C1T.melody = he.h.m3_en_UK_lash.SteiraPsychi.name
peMA.Ode3C1T.incipit = "Your conceiving was without seed…"
peMA.Ode3C1T.text = "Your conceiving was without seed, your child-bearing beyond understanding, O immaculate Virgin-mother; an awesome thing, greatest marvel, feared by Angels, glorified by mortals, O Maiden, Sovereign Lady ."

