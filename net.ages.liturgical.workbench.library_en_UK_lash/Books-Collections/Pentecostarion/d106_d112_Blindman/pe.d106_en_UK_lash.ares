peMA.Ode9C1H.incipit = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.incipit
peMA.Ode9C1H.text = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.text

peMA.Ode9C11.ode = miscellanea_en_UK_lash.misc.Ode9
peMA.Ode9C11.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode9C11.greekmelody = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.greekname
peMA.Ode9C11.melody = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.name
peMA.Ode9C11.incipit = "Joined to the gallows of…"
peMA.Ode9C11.text = "Joined to the gallows of the Cross, Christ Saviour, you triumphed over all the hostile forces of the foe, and abolished the former curse; therefore as is right we magnify you."

peMA.Ode9C12.ode = miscellanea_en_UK_lash.misc.Ode9
peMA.Ode9C12.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode9C12.melody = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.name
peMA.Ode9C12.incipit = "On seeing you below with…"
peMA.Ode9C12.text = "On seeing you below with your soul, O Word, Hell groaned aloud and in fear released all the dead, who recognised the might of your authority; with whom as is right we magnify you."

peMA.Ode9C13.ode = miscellanea_en_UK_lash.misc.Ode9
peMA.Ode9C13.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode9C13.melody = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.name
peMA.Ode9C13.incipit = "Seeing him accomplishing signs and…"
peMA.Ode9C13.text = "Seeing him accomplishing signs and dread wonders the people of the Hebrews in jealousy slew him who plundered Hell by his Resurrection, and as powerful raised all with him."

peMA.Ode9C1F1.ode = miscellanea_en_UK_lash.misc.Ode9
peMA.Ode9C1F1.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode9C1F1.greekmelody = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.greekname
peMA.Ode9C1F1.melody = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.name
peMA.Ode9C1F1.incipit = "Even as you said, you…"
peMA.Ode9C1F1.text = "Even as you said, you rose from the dead, O Giver of Life, and appeared to the holy Disciples after your Rising, you who did signs and gave sight to the blind; with them to the ages we magnify you."

peMA.Ode9C1Tr.ode = miscellanea_en_UK_lash.misc.Ode9
peMA.Ode9C1Tr.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode9C1Tr.melody = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.name
peMA.Ode9C1Tr.incipit = "Praise I give to Light…"
peMA.Ode9C1Tr.text = "Praise I give to Light, the Father, glory to Light, the Son, a hymn to Light, the 'upright Spirit': one undivided Light, recognised in three Persons, God, King of all creation."

peMA.Ode9C1T.ode = miscellanea_en_UK_lash.misc.Ode9
peMA.Ode9C1T.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode9C1T.melody = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.name
peMA.Ode9C1T.incipit = "Holy Virgin, you appear wider…"
peMA.Ode9C1T.text = "Holy Virgin, you appear wider then the heavens for you contained and bore in a body God, the uncircumscribed, for the redemption of all those who sing your praise with undoubting faith."

peMA.Exaposteilarion1.mode = miscellanea_en_UK_lash.misc.Mode2
peMA.Exaposteilarion1.melody = he.a.m2_en_UK_lash.heAU.SarkiYpnosas.notmetered
peMA.Exaposteilarion1.incipit = "Give sight, O Lord, to…"
peMA.Exaposteilarion1.text = "Give sight, O Lord, to the eyes of my soul, maimed by gloomy sin, by grafting in humility, O merciful, and cleansing me with tears of repentance."

peMA.Exaposteilarion2.mode = miscellanea_en_UK_lash.misc.Mode2
peMA.Exaposteilarion2.melody = he.a.m2_en_UK_lash.heAU.GynaikesAkoutisthite.notmetered
peMA.Exaposteilarion2.incipit = "As he passed by, our…"
peMA.Exaposteilarion2.text = "As he passed by, our Saviour found a blind man without sight; spitting upon the ground and making clay he anointed him and sent him to Siloam to go and wash; when he had washed he came seeing your light, my Christ."

