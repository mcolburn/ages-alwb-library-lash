peMA.Ode7C1H.ode = miscellanea_en_UK_lash.misc.Ode7
peMA.Ode7C1H.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode7C1H.greekmelody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.greekname
peMA.Ode7C1H.melody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.melody
peMA.Ode7C1H.incipit = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.incipit
peMA.Ode7C1H.text = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.text

peMA.Ode7C11.ode = miscellanea_en_UK_lash.misc.Ode7
peMA.Ode7C11.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode7C11.greekmelody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.greekname
peMA.Ode7C11.melody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.name
peMA.Ode7C11.incipit = "When you were hung upon…"
peMA.Ode7C11.text = "When you were hung upon the Tree, O Saviour, the sun was quenched, the earth heaved, all creation was shaken, and the dead were raised from the graves."

peMA.Ode7C12.ode = miscellanea_en_UK_lash.misc.Ode7
peMA.Ode7C12.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode7C12.melody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.name
peMA.Ode7C12.incipit = "When you rose from the…"
peMA.Ode7C12.text = "When you rose from the dead, O King, the souls which slept there were raised with you and glorified your sovereign power, through which the bonds of death were loosed."

peMA.Ode7C13.ode = miscellanea_en_UK_lash.misc.Ode7
peMA.Ode7C13.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode7C13.melody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.name
peMA.Ode7C13.incipit = "At dawn a choir of…"
peMA.Ode7C13.text = "At dawn a choir of women came to anoint you, but learning that you had risen, Lord, they rejoiced with the sacred Disciples; through whom grant us redemption from evils."

peMA.Ode7C1F1.ode = miscellanea_en_UK_lash.misc.Ode7
peMA.Ode7C1F1.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode7C1F1.greekmelody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.greekname
peMA.Ode7C1F1.melody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.name
peMA.Ode7C1F1.incipit = "You anointed with clay the…"
peMA.Ode7C1F1.text = "You anointed with clay the eyes of the man born Blind and you ordered him to go to Siloam: having washed he gained his sight as he hymned you, O Christ, King of all."

peMA.Ode7C1Tr.ode = miscellanea_en_UK_lash.misc.Ode7
peMA.Ode7C1Tr.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode7C1Tr.melody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.name
peMA.Ode7C1Tr.incipit = "Let us hymn the Father…"
peMA.Ode7C1Tr.text = "Let us hymn the Father without beginning, the Son likewise without beginning and the all-holy Spirit: Holy, Holy, Holy, are you, O God, King of all."

peMA.Ode7C1T.ode = miscellanea_en_UK_lash.misc.Ode7
peMA.Ode7C1T.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode7C1T.melody = he.h.m5_en_UK_lash.heHE.PyrosSvestirion.name
peMA.Ode7C1T.incipit = "After child-birth you were seen…"
peMA.Ode7C1T.text = "After child-birth you were seen to be a virgin, O Pure one; for you gave birth, O Immaculate, to God who made natures new by his power; ever implore him that we may be saved."

peMA.Ode8C1H.ode = miscellanea_en_UK_lash.misc.Ode8
peMA.Ode8C1H.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode8C1H.greekmelody = he.h.m5_en_UK_lash.heHE.AngelonSystima.greekname
peMA.Ode8C1H.melody = he.h.m5_en_UK_lash.heHE.AngelonSystima.melody
peMA.Ode8C1H.incipit = he.h.m5_en_UK_lash.heHE.AngelonSystima.incipit
peMA.Ode8C1H.text = he.h.m5_en_UK_lash.heHE.AngelonSystima.text

peMA.Ode8C11.ode = miscellanea_en_UK_lash.misc.Ode8
peMA.Ode8C11.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode8C11.greekmelody = he.h.m5_en_UK_lash.heHE.AngelonSystima.greekname
peMA.Ode8C11.melody = he.h.m5_en_UK_lash.heHE.AngelonSystima.name
peMA.Ode8C11.incipit = "The ranks of Angels, seeing…"
peMA.Ode8C11.text = "The ranks of Angels, seeing you hanging upon the Cross, O Christ, universal King, and altering all creation, were struck with fear as they hymned your love for mankind."

peMA.Ode8C12.ode = miscellanea_en_UK_lash.misc.Ode8
peMA.Ode8C12.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode8C12.melody = he.h.m5_en_UK_lash.heHE.AngelonSystima.name
peMA.Ode8C12.incipit = "Hell, seeing you below, groaned…"
peMA.Ode8C12.text = "Hell, seeing you below, groaned aloud and rapidly gave back the dead whom it had held under guard since time began, O Christ, as they hymned your love for mankind."

peMA.Ode8C13.ode = miscellanea_en_UK_lash.misc.Ode8
peMA.Ode8C13.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode8C13.melody = he.h.m5_en_UK_lash.heHE.AngelonSystima.name
peMA.Ode8C13.incipit = "Having accomplished extraordinary marvels, you…"
peMA.Ode8C13.text = "Having accomplished extraordinary marvels, you were willingly lifted up on a cross and joined with the dead, O Christ, and you put Hell to death and with courage released all its prisoners."

peMA.Ode8C1F1.ode = miscellanea_en_UK_lash.misc.Ode8
peMA.Ode8C1F1.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode8C1F1.greekmelody = he.h.m5_en_UK_lash.heHE.AngelonSystima.greekname
peMA.Ode8C1F1.melody = he.h.m5_en_UK_lash.heHE.AngelonSystima.name
peMA.Ode8C1F1.incipit = "You gave eyes to the…"
peMA.Ode8C1F1.text = "You gave eyes to the Blind Man who came to you, O Christ, ordering him to wash in the spring of Siloam, to see and to proclaim you as God, manifest in the flesh for the salvation of the world."

peMA.Ode8C1Tr.ode = miscellanea_en_UK_lash.misc.Ode8
peMA.Ode8C1Tr.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode8C1Tr.melody = he.h.m5_en_UK_lash.heHE.AngelonSystima.name
peMA.Ode8C1Tr.incipit = "O undivided Trinity, unconfused Unity…"
peMA.Ode8C1Tr.text = "O undivided Trinity, unconfused Unity, God of all and creator of all things, save from manifold temptations those who hymn and faithfully worship your might."

peMA.Ode8C1T.ode = miscellanea_en_UK_lash.misc.Ode8
peMA.Ode8C1T.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode8C1T.melody = he.h.m5_en_UK_lash.heHE.AngelonSystima.name
peMA.Ode8C1T.incipit = "Immaculate Virgin, full of God’s…"
peMA.Ode8C1T.text = "Immaculate Virgin, full of God’s grace, ever implore your Son not to put me to shame on the day of judgement, but to number me with the chosen sheep."

peMA.Ode9C1H.ode = miscellanea_en_UK_lash.misc.Ode9
peMA.Ode9C1H.mode = miscellanea_en_UK_lash.misc.Mode5
peMA.Ode9C1H.greekmelody = he.h.m5_en_UK_lash.heHE.OtiEpoiiseSoiMegaleia.greekname
