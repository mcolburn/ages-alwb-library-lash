peVE.Apolytikion1.mode = misc_en_UK_lash.Mode4
peVE.Apolytikion1.melody = ""
peVE.Apolytikion1.text = "You were taken up in glory, Christ our God, making the Disciples joyful by the promise of the Holy Spirit. Through the blessing they were assured that you are the Son of God, the Redeemer of the world."

