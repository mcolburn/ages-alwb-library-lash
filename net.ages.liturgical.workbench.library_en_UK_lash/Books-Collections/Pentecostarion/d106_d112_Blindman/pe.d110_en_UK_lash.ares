peMA.Ode7C1H.ode = misc_en_UK_lash.Ode7
peMA.Ode7C1H.mode = misc_en_UK_lash.Mode5
peMA.Ode7C1H.melody = misc_en_UK_lash.HeirmosNotMetered
peMA.Ode7C1H.text = he.h.m5_en_UK_lash.OEnKaminoPyros.text

peMA.Ode7C11.ode = misc_en_UK_lash.Ode7
peMA.Ode7C11.mode = misc_en_UK_lash.Mode5
peMA.Ode7C11.melody = he.h.m5_en_UK_lash.OEnKaminoPyros.name
peMA.Ode7C11.text = "You were taken up on a cloud of light and saved the world; blessed are you the God of our fathers." 

peMA.Ode7C12.ode = misc_en_UK_lash.Ode7
peMA.Ode7C12.mode = misc_en_UK_lash.Mode5
peMA.Ode7C12.melody = he.h.m5_en_UK_lash.OEnKaminoPyros.name
peMA.Ode7C12.text = "Lifting up on your shoulders the nature which had been led astray, O Christ, you were taken up and brought it to God the Father." 

peMA.Ode7C13.ode = misc_en_UK_lash.Ode7
peMA.Ode7C13.mode = misc_en_UK_lash.Mode5
peMA.Ode7C13.melody = he.h.m5_en_UK_lash.OEnKaminoPyros.name
peMA.Ode7C13.text = "You ascended in the flesh to the bodiless Father; blessed are you the God of our fathers." 

peMA.Ode7C14.ode = misc_en_UK_lash.Ode7
peMA.Ode7C14.mode = misc_en_UK_lash.Mode5
peMA.Ode7C14.melody = he.h.m5_en_UK_lash.OEnKaminoPyros.name
peMA.Ode7C14.text = "Lifting up our nature which had been slain by sin, O Saviour, you brought it to your own Father." 

peMA.Ode7C1T.ode = misc_en_UK_lash.Ode7
peMA.Ode7C1T.mode = misc_en_UK_lash.Mode5
peMA.Ode7C1T.melody = he.h.m5_en_UK_lash.OEnKaminoPyros.name
peMA.Ode7C1T.text = "You were born from a Virgin, whom you made Mother of God; blessed are you the God of our fathers."

peMA.Ode7C2H.ode = misc_en_UK_lash.Ode7
peMA.Ode7C2H.mode = misc_en_UK_lash.Mode4
peMA.Ode7C2H.melody = misc_en_UK_lash.HeirmosNotMetered
peMA.Ode7C2H.text = he.h.m4_en_UK_lash.OukElatrefsan.text

peMA.Ode7C21.ode = misc_en_UK_lash.Ode7
peMA.Ode7C21.mode = misc_en_UK_lash.Mode4
peMA.Ode7C21.melody = he.h.m4_en_UK_lash.OukElatrefsan.name
peMA.Ode7C21.text = "A cloud of light received you, Lord, who are light, as you were being taken up beyond understanding from earth; and heavenly multitudes with the Apostles praised you, saying, 'O God, blessed are you'." 

peMA.Ode7C22.ode = misc_en_UK_lash.Ode7
peMA.Ode7C22.mode = misc_en_UK_lash.Mode4
peMA.Ode7C22.melody = he.h.m4_en_UK_lash.OukElatrefsan.name
peMA.Ode7C22.text = "Let us all gladly clap our hands at Christ's Assumption, and let us shout with joy, 'The Lord has gone up with the sound of the trumpet and, as equal in majesty with the Father, taken his seat on his right hand to the ages'." 

peMA.Ode7C23.ode = misc_en_UK_lash.Ode7
peMA.Ode7C23.mode = misc_en_UK_lash.Mode4
peMA.Ode7C23.melody = he.h.m4_en_UK_lash.OukElatrefsan.name
peMA.Ode7C23.text = "Great Moses of old chanted and cried out: Let the Angels of heaven worship Christ as he ascends as King of all. To him we cry, 'Lord and God of our Fathers, blessed are you'." 

peMA.Ode7C2T.ode = misc_en_UK_lash.Ode7
peMA.Ode7C2T.mode = misc_en_UK_lash.Mode4
peMA.Ode7C2T.melody = he.h.m4_en_UK_lash.OukElatrefsan.name
peMA.Ode7C2T.text = "O marvellous wonders! How did you, O full of God's grace, contain the uncontainable God, who made himself poor according to the flesh, and today with great glory has been taken up to heavenly places and given life to humankind."

peMA.Ode8C1H.ode = misc_en_UK_lash.Ode8
peMA.Ode8C1H.mode = misc_en_UK_lash.Mode5
peMA.Ode8C1H.melody = misc_en_UK_lash.HeirmosNotMetered
peMA.Ode8C1H.text = he.h.m5_en_UK_lash.TonEkPatrosProAionon.text

peMA.Ode8C11.ode = misc_en_UK_lash.Ode8
peMA.Ode8C11.mode = misc_en_UK_lash.Mode5
peMA.Ode8C11.melody = he.h.m5_en_UK_lash.TonEkPatrosProAionon.name
peMA.Ode8C11.text = "Christ, the giver of life, who rose in his two natures with glory to heaven and is now seated with the Father, you priests praise, you people highly exalt to all the ages." 

peMA.Ode8C12.ode = misc_en_UK_lash.Ode8
peMA.Ode8C12.mode = misc_en_UK_lash.Mode5
peMA.Ode8C12.melody = he.h.m5_en_UK_lash.TonEkPatrosProAionon.name
peMA.Ode8C12.text = "Saviour, who redeemed creation from the slavery of idols and set it free before your own Father, we praise you and highly exalt you to all the ages." 

peMA.Ode8C13.ode = misc_en_UK_lash.Ode8
peMA.Ode8C13.mode = misc_en_UK_lash.Mode5
peMA.Ode8C13.melody = he.h.m5_en_UK_lash.TonEkPatrosProAionon.name
peMA.Ode8C13.text = "The One who by his descent threw down the opponent and by his ascent exalted humankind, you priests praise, you people highly exalt to all the ages." 

peMA.Ode8C1T.ode = misc_en_UK_lash.Ode8
peMA.Ode8C1T.mode = misc_en_UK_lash.Mode5
peMA.Ode8C1T.melody = he.h.m5_en_UK_lash.TonEkPatrosProAionon.name
peMA.Ode8C1T.text = "You were revealed, pure Mother of God, as higher than the Cherubim, for you carried in your womb the One who rides upon them; whom with the bodiless powers we mortals glorify to all the ages."

peMA.Ode8C2H.ode = misc_en_UK_lash.Ode8
peMA.Ode8C2H.mode = misc_en_UK_lash.Mode4
peMA.Ode8C2H.melody = misc_en_UK_lash.HeirmosNotMetered
peMA.Ode8C2H.text = he.h.m4_en_UK_lash.PaidasEvageis.text

peMA.Ode8C21.ode = misc_en_UK_lash.Ode8
peMA.Ode8C21.mode = misc_en_UK_lash.Mode4
peMA.Ode8C21.melody = he.h.m4_en_UK_lash.PaidasEvageis.name
peMA.Ode8C21.text = "The Intelligences appeared to the Apostles at the Assumption and said, 'Why do you stand gazing and amazed? He who is ascending into heaven will also come again to judge humankind on earth, for he alone is judge'." 

peMA.Ode8C22.ode = misc_en_UK_lash.Ode8
peMA.Ode8C22.mode = misc_en_UK_lash.Mode4
peMA.Ode8C22.melody = he.h.m4_en_UK_lash.PaidasEvageis.name
peMA.Ode8C22.text = "Let us give majesty to God; let us cry aloud with one accord; let us sing; let us dance and let us clap our hands. Our God has gone up from earth to heaven, as Angels and Archangels sing his praise as Master and Maker of all things." 

peMA.Ode8C23.ode = misc_en_UK_lash.Ode8
peMA.Ode8C23.mode = misc_en_UK_lash.Mode4
peMA.Ode8C23.melody = he.h.m4_en_UK_lash.PaidasEvageis.name
peMA.Ode8C23.text = "Our nature, which of old had fallen, has been raised above the Angels and beyond understanding established on God's throne. Come, let us keep festival and let us cry out, 'You his works, praise the Lord, and highly exalt him to all the ages'." 

peMA.Ode8C2T.ode = misc_en_UK_lash.Ode8
peMA.Ode8C2T.mode = misc_en_UK_lash.Mode4
peMA.Ode8C2T.melody = he.h.m4_en_UK_lash.PaidasEvageis.name
peMA.Ode8C2T.text = "See, Mother of God, your Son, having despoiled death by his Cross, rose on the third day, and having shown himself to his Disciples he has hastened to the heavenly places; as we worship you with him, we give praise and glory to all the ages."

peMA.Ode9C1H.ode = misc_en_UK_lash.Ode9
peMA.Ode9C1H.mode = misc_en_UK_lash.Mode5
peMA.Ode9C1H.melody = misc_en_UK_lash.HeirmosNotMetered
peMA.Ode9C1H.text = he.h.m5_en_UK_lash.SeTinYperNoun.text

peMA.Ode9C11.ode = misc_en_UK_lash.Ode9
peMA.Ode9C11.mode = misc_en_UK_lash.Mode5
peMA.Ode9C11.melody = he.h.m5_en_UK_lash.SeTinYperNoun.name
peMA.Ode9C11.text = "As they watched you, Christ God, the redeemer of the world, being divinely exalted, the Apostles, leaping for joy, magnified you with awe." 

peMA.Ode9C12.ode = misc_en_UK_lash.Ode9
peMA.Ode9C12.mode = misc_en_UK_lash.Mode5
peMA.Ode9C12.melody = he.h.m5_en_UK_lash.SeTinYperNoun.name
peMA.Ode9C12.text = "As the Angels saw in the height your flesh now made divine, O Christ, they signalled to one another: Truly this is our God." 

peMA.Ode9C13.ode = misc_en_UK_lash.Ode9
peMA.Ode9C13.mode = misc_en_UK_lash.Mode5
peMA.Ode9C13.melody = he.h.m5_en_UK_lash.SeTinYperNoun.name
peMA.Ode9C13.text = "As the ranks of bodiless powers, Christ God, saw you being lifted up on clouds, they cried: For the King of glory lift up the gates." 

peMA.Ode9C14.ode = misc_en_UK_lash.Ode9
peMA.Ode9C14.mode = misc_en_UK_lash.Mode5
peMA.Ode9C14.melody = he.h.m5_en_UK_lash.SeTinYperNoun.name
peMA.Ode9C14.text = "As the One who descended into the lowest part of the earth, saved humankind and raised it up by your ascension, we magnify you." 

peMA.Ode9C1T.ode = misc_en_UK_lash.Ode9
peMA.Ode9C1T.mode = misc_en_UK_lash.Mode5
peMA.Ode9C1T.melody = he.h.m5_en_UK_lash.SeTinYperNoun.name
peMA.Ode9C1T.text = "
Hail, Mother of God, Mother of Christ God; as today you saw the One you bore flying up from earth, with Angels you magnified him."

peMA.Ode9C2M01.text = "Angels, seeing the ascent of the Master, were amazed at how he was raised with glory from earth to things above."

peMA.Ode9C2H.ode = misc_en_UK_lash.Ode9
peMA.Ode9C2H.mode = misc_en_UK_lash.Mode4
peMA.Ode9C2H.melody = misc_en_UK_lash.HeirmosNotMetered
peMA.Ode9C2H.text = he.h.m4_en_UK_lash.ApasGigenis.text

peMA.Ode9C21.ode = misc_en_UK_lash.Ode9
peMA.Ode9C21.mode = misc_en_UK_lash.Mode4
peMA.Ode9C21.melody = he.h.m4_en_UK_lash.ApasGigenis.name
peMA.Ode9C21.text = "O gifts beyond understanding! O dread mystery! For the Master of all things, rising from earth to heaven, sent to his Disciples the Holy Spirit, who enlightened their minds and set them afire with grace." 

peMA.Ode9C22.ode = misc_en_UK_lash.Ode9
peMA.Ode9C22.mode = misc_en_UK_lash.Mode4
peMA.Ode9C22.melody = he.h.m4_en_UK_lash.ApasGigenis.name
peMA.Ode9C22.text = "To the band of Disciples the Lord said: Stay in Jerusalem, and I shall send you another Advocate, equal in majesty with the Father, and equal in honour with me, whom you see being taken up and riding upon a cloud of light." 

peMA.Ode9C23.ode = misc_en_UK_lash.Ode9
peMA.Ode9C23.mode = misc_en_UK_lash.Mode4
peMA.Ode9C23.melody = he.h.m4_en_UK_lash.ApasGigenis.name
peMA.Ode9C23.text = "The majesty of him who became poor in the flesh has been raised above the heavens and our fallen nature honoured by sitting with the Father. Let us keep festival and all cry aloud with one accord, and gladly clap our hands." 

peMA.Ode9C2T.ode = misc_en_UK_lash.Ode9
peMA.Ode9C2T.mode = misc_en_UK_lash.Mode4
peMA.Ode9C2T.melody = he.h.m4_en_UK_lash.ApasGigenis.name
peMA.Ode9C2T.text = "The Light which shone from Light dawned from you, dispelled all the blackness of atheism and led to the light those who slept in night; and so, as is fitting, to the ages we ever call you blessed."

peMA.Exaposteilarion1.mode = misc_en_UK_lash.Mode2
peMA.Exaposteilarion1.melody = misc_en_UK_lash.AutomelonNotMetered
peMA.Exaposteilarion1.text = he.a.m2_en_UK_lash.TonMathitonOrontonSe.text

peMA.Lauds1.mode = misc_en_UK_lash.Mode1
peMA.Lauds1.melody = he.a.m1_en_UK_lash.TonOuranionTagmaton.notmetered
peMA.Lauds1.text = "Let us in the world keep festival like the Angels, for God who is borne upon a throne of glory, as we cry aloud the hymn: Holy are you, heavenly Father; holy are you, co-eternal Son; holy are you, all-holy Spirit."

peMA.Lauds2.mode = misc_en_UK_lash.Mode1
peMA.Lauds2.melody = he.a.m1_en_UK_lash.TonOuranionTagmaton.notmetered
peMA.Lauds2.text = "The princes of the Angels, Saviour, as they saw the strangeness of your ascent, were perplexed and said to one another, 'What is this sight? Human he seems in form, yet as God he ascends with a body far above the heavens'."

peMA.Lauds3.mode = misc_en_UK_lash.Mode1
peMA.Lauds3.melody = he.a.m1_en_UK_lash.TonOuranionTagmaton.notmetered
peMA.Lauds3.text = "The men of Galilee, seeing you, O Word, taken up from the mount of Olives with a body, heard Angels crying out to them, 'Why do you stand gazing? He will come again in the flesh in the same way that you have seen him'."

