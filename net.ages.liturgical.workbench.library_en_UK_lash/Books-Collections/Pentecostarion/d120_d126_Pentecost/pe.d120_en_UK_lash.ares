peMA.Ode8C21.melody = he.h.m4_en_UK_lash.LyeiTaDesma.name
peMA.Ode8C21.incipit = "Coming to rest in the…"
peMA.Ode8C21.text = "Coming to rest in the form of tongues of fire, * The Spirit, wrought remembrance of the saving words * Which Christ heard from the Father and told * To the Apostles. Creation, once estranged, * Now reconciled, sings of you as blessed."
peMA.Ode8C211.text = "Coming to rest in the form of tongues of fire,"
peMA.Ode8C212.text = "The Spirit, wrought remembrance of the saving words"
peMA.Ode8C213.text = "Which Christ heard from the Father and told"
peMA.Ode8C214.text = "To the Apostles. Creation, once estranged,"
peMA.Ode8C215.text = "Now reconciled, sings of you as blessed."

peMA.Ode8C22.ode = misc_en_UK_lash.Ode8
peMA.Ode8C22.mode = misc_en_UK_lash.Mode4
peMA.Ode8C22.melody = he.h.m4_en_UK_lash.LyeiTaDesma.name
peMA.Ode8C22.incipit = "As Saviour, coming of your…"
peMA.Ode8C22.text = "As Saviour, coming of your own authority, * Light shining of itself and provider of light, * You came as a precious wind; * While to your servants you distribute * The Spirit so earnestly prayed for."
peMA.Ode8C221.text = "As Saviour, coming of your own authority,"
peMA.Ode8C222.text = "Light shining of itself and provider of light,"
peMA.Ode8C223.text = "You came as a precious wind;"
peMA.Ode8C224.text = "While to your servants you distribute"
peMA.Ode8C225.text = "The Spirit so earnestly prayed for."

peMA.Ode8C23.ode = misc_en_UK_lash.Ode8
peMA.Ode8C23.mode = misc_en_UK_lash.Mode4
peMA.Ode8C23.melody = he.h.m4_en_UK_lash.LyeiTaDesma.name
peMA.Ode8C23.incipit = "The spirit-filled mouth of Prophets…"
peMA.Ode8C23.text = "The spirit-filled mouth of Prophets sang * Of your sojourning, high Lord, in bodily form. * And your Spirit proceeding from the Father's bosom, * Uncreated-cofashioning-cothroned, * You send to believers for the worship of the incarnation."
peMA.Ode8C231.text = "The spirit-filled mouth of Prophets sang"
peMA.Ode8C232.text = "Of your sojourning, high Lord, in bodily form."
peMA.Ode8C233.text = "And your Spirit proceeding from the Father's bosom,"
peMA.Ode8C234.text = "Uncreated-cofashioning-cothroned,"
peMA.Ode8C235.text = "You send to believers for the worship of the incarnation."

peMA.Ode9C1H.ode = misc_en_UK_lash.Ode9
peMA.Ode9C1H.mode = misc_en_UK_lash.Mode7
