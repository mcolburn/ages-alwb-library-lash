peMA.Ode6C21.melody = he.h.m4_en_UK_lash.IlasmosIminChriste.name
peMA.Ode6C21.incipit = "Almighty, renew a cherished…"
peMA.Ode6C21.text = "Almighty, renew a cherished, a right Spirit * Within us, to hold it eternally, * Who is ever united and proceeding from the Father, * Purging minds of hateful matter, of burning * Defilements and filth."
peMA.Ode6C211.text = "Almighty, renew a cherished, a right Spirit"
peMA.Ode6C212.text = "Within us, to hold it eternally,"
peMA.Ode6C213.text = "Who is ever united and proceeding from the Father,"
peMA.Ode6C214.text = "Purging minds of hateful matter, of burning"
peMA.Ode6C215.text = "Defilements and filth."

peMA.Ode6C22.ode = misc_en_UK_lash.Ode6
peMA.Ode6C22.mode = misc_en_UK_lash.Mode4
peMA.Ode6C22.melody = he.h.m4_en_UK_lash.IlasmosIminChriste.name
peMA.Ode6C22.incipit = "For the Apostles who awaited…"
peMA.Ode6C22.text = "For the Apostles who awaited your coming * In Sion you establish with fiery wind * A longed for dignity, O Spirit, * Knowledge of the Word begotten of the Father, * Swiftly exposing the wild chatter of the nations' babblings."
peMA.Ode6C221.text = "For the Apostles who awaited your coming"
peMA.Ode6C222.text = "In Sion you establish with fiery wind"
peMA.Ode6C223.text = "A longed for dignity, O Spirit,"
peMA.Ode6C224.text = "Knowledge of the Word begotten of the Father,"
peMA.Ode6C225.text = "Swiftly exposing the wild chatter of the nations' babblings."

peMA.Kontakion1.mode = misc_en_UK_lash.Mode8
peMA.Kontakion1.melody = ""
peMA.Kontakion1.incipit = "When the Most High came…"
peMA.Kontakion1.text = "When the Most High came down he confused the tongues, divided the nations; but when he parted the tongues of fire, he called all to unity, and with one voice we glorify the all-holy Spirit."

peMA.Oikos1.incipit = "…"
peMA.Oikos1.text = "Give swift and stable comfort to your servants, Jesu, in the despondency of our spirits; do not part from our souls in troubles, do not be far from our minds in perils, but ever anticipate us. Be near us, be near you, who are everywhere; as you are also always with your Apostles, so unite yourself, O compassionate, with those who long for you, that united to you we may hymn and glorify your All-holy Spirit."

peSY.commemoration.text = "On the same day, the eighth Sunday from Pascha, we celebrate the holy Pentecost."
peSY.commemoration.couplet1.title = titles_en_UK_lash.ti.Verses
peSY.commemoration.couplet1.verse1 = "With mighty wind, in form like tongues of fire"
peSY.commemoration.couplet1.verse2 = "Christ gives God's Spirit now to the Apostles."
peSY.commemoration.single1 = "Spirit on this great day on fishers poured."
peSY.commemoration.narration.p01 = "[paragraph 01]"
peSY.commemoration.narration.p02 = "[paragraph 02]"
peSY.commemoration.narration.p03 = "[paragraph 03]"
peSY.commemoration.narration.p04 = "[paragraph 04]"
peSY.commemoration.narration.p05 = "[paragraph 05]"
peSY.commemoration.narration.p06 = "[paragraph 06]"
peSY.commemoration.narration.p07 = "[paragraph 07]"
peSY.lastline = "At the prayers of the holy Apostles, Christ our God, have mercy on us. Amen."

