peMA.Ode5C21.incipit = "As he was well-pleased…"
peMA.Ode5C21.text = "As he was well-pleased of his own authority * The un-mastered Spirit comes down from the Father, * Making the Apostles wise with tongues, * Might of the Father, one in form, setting the seal * Upon the life-bearing word, which the Saviour spoke."
peMA.Ode5C211.text = "As he was well-pleased of his own authority"
peMA.Ode5C212.text = "The un-mastered Spirit comes down from the Father,"
peMA.Ode5C213.text = "Making the Apostles wise with tongues,"
peMA.Ode5C214.text = "Might of the Father, one in form, setting the seal"
peMA.Ode5C215.text = "Upon the life-bearing word, which the Saviour spoke."

peMA.Ode5C22.ode = misc_en_UK_lash.Ode5
peMA.Ode5C22.mode = misc_en_UK_lash.Mode4
