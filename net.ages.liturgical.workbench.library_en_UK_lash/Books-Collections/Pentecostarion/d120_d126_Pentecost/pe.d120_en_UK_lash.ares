peMA.Ode7C21.melody = he.h.m4_en_UK_lash.SymfononEthroisen.name
peMA.Ode7C21.incipit = "Fools did not recognise the…"
peMA.Ode7C21.text = "Fools did not recognise the voice spoken * In prophecy, called it drunkenness from wine, * When they heard the Apostles sayings in strange tongues; * We the devout, inspired, cry out to you: * Renewer of the universe, blessed are you."
peMA.Ode7C211.text = "Fools did not recognise the voice spoken"
peMA.Ode7C212.text = "In prophecy, called it drunkenness from wine,"
peMA.Ode7C213.text = "When they heard the Apostles sayings in strange tongues;"
peMA.Ode7C214.text = "We the devout, inspired, cry out to you:"
peMA.Ode7C215.text = "Renewer of the universe, blessed are you."

peMA.Ode7C22.ode = misc_en_UK_lash.Ode7
peMA.Ode7C22.mode = misc_en_UK_lash.Mode4
peMA.Ode7C22.melody = he.h.m4_en_UK_lash.SymfononEthroisen.name
peMA.Ode7C22.incipit = "The seer Joel, inspired, thundered…"
peMA.Ode7C22.text = "The seer Joel, inspired, thundered an oracle * Spoken by the divine Word: * Those on whom I pour my Spirit * Will cry out together: Nature, * Shining with threefold splendour, blessed are you."
peMA.Ode7C221.text = "The seer Joel, inspired, thundered an oracle"
peMA.Ode7C222.text = "Spoken by the divine Word:"
peMA.Ode7C223.text = "Those on whom I pour my Spirit"
peMA.Ode7C224.text = "Will cry out together: Nature,"
peMA.Ode7C225.text = "Shining with threefold splendour, blessed are you."

peMA.Ode7C23.ode = misc_en_UK_lash.Ode7
peMA.Ode7C23.mode = misc_en_UK_lash.Mode4
peMA.Ode7C23.melody = he.h.m4_en_UK_lash.SymfononEthroisen.name
peMA.Ode7C23.incipit = "The third hour was made…"
peMA.Ode7C23.text = "The third hour was made rich with grace, * That it might indicate the worship of * Three Persons in singleness of authority; * But now on the one Sovereign of days, * Son, Father, Spirit, blessed are you."
peMA.Ode7C231.text = "The third hour was made rich with grace,"
peMA.Ode7C232.text = "That it might indicate the worship of"
peMA.Ode7C233.text = "Three Persons in singleness of authority;"
peMA.Ode7C234.text = "But now on the one Sovereign of days,"
peMA.Ode7C235.text = "Son, Father, Spirit, blessed are you."

