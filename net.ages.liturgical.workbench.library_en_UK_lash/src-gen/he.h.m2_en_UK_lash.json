{'id_' : 'he|h|m2_en_UK_lash|KaminosPote|name', 'text' : 'A furnace once. (NM)'}
{'id_' : 'he|h|m2_en_UK_lash|KaminosPote|mode', 'text' : 'Mode 2.'}
