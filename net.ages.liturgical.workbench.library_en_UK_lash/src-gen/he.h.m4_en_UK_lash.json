{'id_' : 'he|h|m4_en_UK_lash|LithosAcheirotmitos|name', 'text' : 'A Stone not cut. (NM)'}
{'id_' : 'he|h|m4_en_UK_lash|LithosAcheirotmitos|notmetered', 'text' : 'N/M (A cornerstone.)'}
{'id_' : 'he|h|m4_en_UK_lash|LithosAcheirotmitos|mode', 'text' : 'Mode 4.'}
