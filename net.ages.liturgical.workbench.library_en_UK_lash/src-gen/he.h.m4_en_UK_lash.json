{'id_' : 'he|h|m4_en_UK_lash|AnaxAnakton|name', 'text' : 'Monarch of monarchs.'}
{'id_' : 'he|h|m4_en_UK_lash|AnaxAnakton|mode', 'text' : 'Mode 4.'}
