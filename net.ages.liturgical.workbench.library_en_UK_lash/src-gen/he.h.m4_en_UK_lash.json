{'id_' : 'he|h|m4_en_UK_lash|LytirionKatharsin|name', 'text' : 'O light-formed children.'}
{'id_' : 'he|h|m4_en_UK_lash|LytirionKatharsin|mode', 'text' : 'Mode 4.'}
