{'id_' : 'ho|s23_en_UK_lash|hoCA|Troparion2|mode', 'text' : 'Mode pl. 2.'}
{'id_' : 'ho|s23_en_UK_lash|hoCA|Troparion2|melody', 'text' : ''}
