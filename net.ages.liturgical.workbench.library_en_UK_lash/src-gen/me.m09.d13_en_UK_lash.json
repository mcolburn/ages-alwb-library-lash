{'id_' : 'me|m09|d13_en_UK_lash|meShortCommemoration1|text', 'text' : 'Commemoration of the dedication of the of the Holy [Church of the] Resurrection of Christ our God'}
{'id_' : 'me|m09|d13_en_UK_lash|meShortCommemoration2|text', 'text' : 'Forefeast of the Exaltation of the precious and life-giving Cross'}
{'id_' : 'me|m09|d13_en_UK_lash|meCom1|title', 'text' : ''}
{'id_' : 'me|m09|d13_en_UK_lash|meCom2|title', 'text' : ''}
