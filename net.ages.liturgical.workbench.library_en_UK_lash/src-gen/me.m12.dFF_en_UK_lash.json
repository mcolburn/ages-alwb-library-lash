{'id_' : 'me|m12|dFF_en_UK_lash|meCommemoration|text', 'text' : 'Sunday of the Holy Forefathers'}
{'id_' : 'me|m12|dFF_en_UK_lash|meShortCommemoration1|text', 'text' : ''}
{'id_' : 'me|m12|dFF_en_UK_lash|meCom1|title', 'text' : ''}
