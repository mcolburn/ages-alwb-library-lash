{'id_' : 'pe|d078_en_UK_lash|peMA|PostGospelBoth|mode', 'text' : 'Mode 2.'}
{'id_' : 'pe|d078_en_UK_lash|peMA|PostGospelBoth|melody', 'text' : ''}
{'id_' : 'pe|d078_en_UK_lash|peMA|PostGospelBoth|text', 'text' : 'At the prayers of the Mother of God O Merciful One, wipe away the multitude of my offences.'}
{'id_' : 'pe|d078_en_UK_lash|peMA|PostGospelIdiomelon|mode', 'text' : 'Mode 2.'}
