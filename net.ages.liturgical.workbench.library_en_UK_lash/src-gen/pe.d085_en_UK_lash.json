{'id_' : 'pe|d085_en_UK_lash|peVE|SticGlory|mode', 'text' : 'Mode pl. 2.'}
{'id_' : 'pe|d085_en_UK_lash|peVE|SticGlory|melody', 'text' : 'Idiomel'}
